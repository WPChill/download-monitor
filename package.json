{
  "name": "download-monitor",
  "title": "Download Monitor",
  "version": "5.1.0",
  "homepage": "https://www.download-monitor.com",
  "main": "Gruntfile.js",
  "devDependencies": {
    "@babel/core": "^7.24.6",
    "@babel/eslint-parser": "^7.24.6",
    "@floatwork/grunt-po2mo": "^0.3.0",
    "@tanstack/eslint-plugin-query": "^5.62.1",
    "@wordpress/babel-preset-default": "^7.42.0",
    "@wordpress/block-editor": "^12.26.0",
    "@wordpress/eslint-plugin": "^18.1.0",
    "@wordpress/prettier-config": "^3.15.0",
    "@wordpress/scripts": "^27.9.0",
    "@wordpress/stylelint-config": "^21.41.0",
    "archiver": "^5.3.1",
    "cross-env": "^7.0.3",
    "cypress": "^10.3.0",
    "fs-extra": "^11.2.0",
    "glob": "^11.0.0",
    "grunt": "^1.5.3",
    "grunt-checktextdomain": "^1.0.1",
    "grunt-contrib-clean": "^2.0.1",
    "grunt-contrib-compress": "^2.0.0",
    "grunt-contrib-concat": "^2.1.0",
    "grunt-contrib-copy": "^1.0.0",
    "grunt-contrib-cssmin": "~4.0.0",
    "grunt-contrib-less": "~3.0.0",
    "grunt-contrib-rename": "^0.2.0",
    "grunt-contrib-uglify": "~5.2.2",
    "grunt-contrib-watch": "~1.1.0",
    "grunt-wp-i18n": "^1.0.3",
    "load-grunt-tasks": "^5.1.0",
    "rimraf": "^5.0.1",
    "tailwindcss": "^3.3.3"
  },
  "engines": {
    "node": ">=0.8.0",
    "npm": ">=1.1.0"
  },
  "scripts": {
    "build:start": "npm run build && npm run build:notification-system && npm run build:reports && npm run build:reports-widget && npm run build:upsells",
    "plugin-zip": "npm run build:start && npm run clean && npm run copy && npm run bundle && npm run clean",
    "start:notification-system": "wp-scripts start --config webpack.config.notification-system.js",
    "build:notification-system": "wp-scripts build --config webpack.config.notification-system.js",
    "start:reports": "wp-scripts start --config webpack.config.reports.js",
    "build:reports": "wp-scripts build --config webpack.config.reports.js",
    "start:upsells": "wp-scripts start --config webpack.config.upsells.js",
    "build:upsells": "wp-scripts build --config webpack.config.upsells.js",
    "start:reports-widget": "wp-scripts start --config webpack.config.reports-widget.js",
    "build:reports-widget": "wp-scripts build --config webpack.config.reports-widget.js",
    "build": "wp-scripts build ./assets/blocks/src/blocks.js --output-path=./assets/blocks/dist",
    "start": "wp-scripts start ./assets/blocks/src/blocks.js --output-path=./assets/blocks/dist",
    "packages-update": "wp-scripts packages-update",
    "cypress:open": "./node_modules/.bin/cypress open",
    "composer:install": "composer install && \"vendor/bin/mozart\" compose && composer update --no-dev composer dump-autoload",
    "composer:update": "composer update && \"vendor/bin/mozart\" compose && composer update --no-dev composer dump-autoload",
    "clean": "rimraf build",
    "copy": "node scripts/copy.js",
    "bundle": "node scripts/bundle.js"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.52.0",
    "@tanstack/react-table": "^8.21.3",
    "@wordpress/api-fetch": "^7.5.0",
    "@wordpress/block-editor": "^14.0.0",
    "@wordpress/blocks": "^13.5.0",
    "@wordpress/components": "^28.5.0",
    "@wordpress/data": "^10.5.0",
    "@wordpress/element": "^6.5.0",
    "@wordpress/hooks": "^4.26.0",
    "@wordpress/i18n": "^5.7.0",
    "dayjs": "^1.11.13",
    "he": "^1.2.0",
    "interweave": "^13.1.0",
    "lucide-react": "^0.525.0",
    "react-select": "^5.3.2",
    "recharts": "^3.0.2",
    "whatwg-fetch": "^3.6.2"
  }
}
